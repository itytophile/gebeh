{
  "compilerOptions": {
    "noImplicitAny": true,
    "allowJs": true,
    "sourceMap": true,
    "module": "esnext",
    "target": "esnext",
    "jsx": "react",
    "moduleResolution": "bundler",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop" : true,
    "outDir": "./dist",
  },
  "include": ["ts"],
}
